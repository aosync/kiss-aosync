<td class='lines'><pre><code>Use monotonic clock for pthread_cond_timedwait with musl too.

<span style="color:#000000; font-weight:bold">--- ./v8/src/base/platform/condition-variable.cc</span>
<span style="color:#0057ae">+++ ./v8/src/base/platform/condition-variable.cc</span>
<span style="color:#010181">&#64;&#64; -16,7 +16,7 &#64;&#64;</span>
 
 ConditionVariable::ConditionVariable() {
 #if (V8_OS_FREEBSD || V8_OS_NETBSD || V8_OS_OPENBSD || \
<span style="color:#000000; font-weight:bold">-     (V8_OS_LINUX &amp;&amp; V8_LIBC_GLIBC))</span>
<span style="color:#0057ae">+     V8_OS_LINUX)</span>
   // On Free/Net/OpenBSD and Linux with glibc we can change the time
   // source for pthread_cond_timedwait() to use the monotonic clock.
   pthread_condattr_t attr;
<span style="color:#010181">&#64;&#64; -92,7 +92,7 &#64;&#64;</span>
       &amp;native_handle_, &amp;mutex-&gt;native_handle(), &amp;ts);
 #else
 #if (V8_OS_FREEBSD || V8_OS_NETBSD || V8_OS_OPENBSD || \
<span style="color:#000000; font-weight:bold">-     (V8_OS_LINUX &amp;&amp; V8_LIBC_GLIBC))</span>
<span style="color:#0057ae">+     V8_OS_LINUX)</span>
   // On Free/Net/OpenBSD and Linux with glibc we can change the time
   // source for pthread_cond_timedwait() to use the monotonic clock.
   result = clock_gettime(CLOCK_MONOTONIC, &amp;ts);
